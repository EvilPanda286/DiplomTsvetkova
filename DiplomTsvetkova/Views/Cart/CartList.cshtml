@model List<Storage>
<form method="post" data="@Model">
    <div>

        @foreach (var storage in Model)
        {
            <h1>
                @storage.Name
            </h1>
            <div>
                <div>
                    @foreach (var product in storage.Products)
                    {
                        <div>
                            @product.Name

                        </div>
                    }
                </div>
            </div>
        }
    </div>











    <p>
        <a href="/Cart/CartList/@Model">Купить</a>
       @* <input type="submit" value="Отправить" />*@
    </p>
</form>
